{
    "name": "@solum/capacitor-msauth",
    "version": "1.0.0",
    "description": "Microsoft MSAL Capacitor plugin for Solum â€” drop-in replacement for @recognizebv/capacitor-plugin-msauth with auto Android manifest injection and production iOS bug fixes.",
    "main": "dist/plugin.js",
    "module": "dist/esm/index.js",
    "types": "dist/esm/index.d.ts",
    "unpkg": "dist/plugin.js",
    "author": "Solum Mobile App Team",
    "license": "MIT",
    "repository": {
        "type": "git",
        "url": "https://github.com/solumApps/solum-msal-capacitor-plugin.git"
    },
    "bugs": {
        "url": "https://github.com/solumApps/solum-msal-capacitor-plugin/issues"
    },
    "homepage": "https://github.com/solumApps/solum-msal-capacitor-plugin#readme",
    "scripts": {
        "build": "npm run clean && tsc && rollup -c rollup.config.mjs",
        "clean": "rimraf ./dist",
        "watch": "rollup -c rollup.config.mjs --watch",
        "prepublishOnly": "npm run build"
    },
    "devDependencies": {
        "@capacitor/android": "^7.0.0",
        "@capacitor/core": "^7.0.0",
        "@capacitor/ios": "^7.0.0",
        "@rollup/plugin-node-resolve": "^15.0.0",
        "rimraf": "^5.0.0",
        "rollup": "^4.0.0",
        "typescript": "~5.6.3"
    },
    "peerDependencies": {
        "@capacitor/core": "^7.0.0"
    },
    "files": [
        "android/src/main/",
        "android/build.gradle",
        "dist/",
        "ios/Plugin/",
        "SolumMsauth.podspec"
    ],
    "keywords": [
        "capacitor",
        "plugin",
        "msal",
        "microsoft",
        "azure",
        "b2c",
        "auth",
        "solum",
        "ionic"
    ],
    "capacitor": {
        "ios": {
            "src": "ios"
        },
        "android": {
            "src": "android"
        },
        "variables": {
            "MSAUTH_KEY_HASH": {
                "description": "Base64-encoded SHA1 hash of your Android signing keystore. Get it with: keytool -exportcert -alias YOUR_ALIAS -keystore YOUR.keystore -storepass PASS | openssl sha1 -binary | openssl base64"
            }
        }
    }
}